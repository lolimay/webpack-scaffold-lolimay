{
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"commonjs": true
	},
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			4
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		],
		"no-console": [
			"off"
		],
		"no-unused-vars":"warn"
	}
}